<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liqg.demo.dao.BookDao">
    <select id="getBook" parameterType="int" resultType="com.liqg.demo.domain.Book">
        select * from book where id = #{id}
    </select>

    <select id="getAllBooks" resultType="com.liqg.demo.domain.Book">
        SELECT * FROM book
    </select>

</mapper>